inductive rgb : top =>
| mk-rgb(r g b : number)

inductive color : top =>
| red
| green
| blue
| mix(v : rgb)

def to-rgb(c : color) : rgb =>
  match c with
  | red => mk-rgb(255, 0, 0)
  | green => mk-rgb(0, 255, 0)
  | blue => mk-rgb(0, 0, 255)
  | mix(?rgb) => rgb

def colors : array(rgb) =>
  [ to-rgb(red)
  , to-rgb(blue)
  , to-rgb(mix(mk-rgb(10, 20, 30)))
  ]